{
  "name": "mll-lab/laravel-db-ready",
  "description": "An artisan command to check if a database connection is ready",
  "type": "library",
  "keywords": ["laravel", "php", "database"],
  "license": "MIT",
  "authors": [
    {
      "name": "Benedikt Franke",
      "email": "benedikt@franke.tech"
    }
  ],
  "require": {
    "php": "^7.2 || ^8",
    "illuminate/contracts": "^6 || ^7 || ^8"
  },
  "require-dev": {
    "orchestra/database": "^4 || ^5 || ^6",
    "orchestra/testbench": "^4 || ^5 || ^6",
    "nunomaduro/larastan": "^0.6"
  },
  "autoload": {
    "psr-4": {
      "MLL\\LaravelDbReady\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MLL\\LaravelDbReady\\LaravelDbReadyServiceProvider"
      ]
    }
  }
}
