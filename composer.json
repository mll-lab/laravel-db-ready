{
  "name": "mll-lab/laravel-db-ready",
  "description": "An artisan command to check if a database connection is ready",
  "type": "library",
  "keywords": ["laravel", "php", "database"],
  "license": "MIT",
  "authors": [
    {
      "name": "Benedikt Franke",
      "email": "benedikt@franke.tech"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/contracts": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0"
  },
  "require-dev": {
    "orchestra/database": "3.5.*|3.6.*|3.7.*|3.8.*|3.9.*",
    "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*|3.9.*",
    "nunomaduro/larastan": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "MLL\\LaravelDbReady\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "stan": "phpstan analyse"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MLL\\LaravelDbReady\\LaravelDbReadyServiceProvider"
      ]
    }
  }
}
