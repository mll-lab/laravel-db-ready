{
  "name": "mll-lab/laravel-db-ready",
  "description": "An artisan command to check if a database connection is ready",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "laravel",
    "php",
    "database"
  ],
  "authors": [
    {
      "name": "Benedikt Franke",
      "email": "benedikt@franke.tech"
    }
  ],
  "require": {
    "php": "^7.4 || ^8",
    "illuminate/contracts": "^8 || ^9"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.23",
    "mll-lab/php-cs-fixer-config": "^4.4",
    "nunomaduro/larastan": "^0.7 || ^1",
    "orchestra/testbench": "^6 || ^7"
  },
  "autoload": {
    "psr-4": {
      "MLL\\LaravelDbReady\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MLL\\LaravelDbReady\\LaravelDbReadyServiceProvider"
      ]
    }
  }
}
